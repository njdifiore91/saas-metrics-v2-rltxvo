{
  "name": "@startup-metrics/auth-service",
  "version": "1.0.0",
  "description": "Secure authentication microservice implementing Google OAuth, JWT authentication, and comprehensive security measures",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon src/app.ts",
    "build": "tsc",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageThreshold='{\"global\":{\"branches\":80,\"functions\":80,\"lines\":80,\"statements\":80}}'",
    "lint": "eslint . --ext .ts --max-warnings 0",
    "format": "prettier --write \"src/**/*.ts\"",
    "audit": "npm audit --audit-level=moderate",
    "prepare": "husky install"
  },
  "dependencies": {
    "@prisma/client": "4.0.0",
    "bcrypt": "5.1.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "express-rate-limit": "6.7.0",
    "google-auth-library": "8.0.0",
    "helmet": "6.0.0",
    "http-status-codes": "2.2.0",
    "ioredis": "5.3.0",
    "joi": "17.7.0",
    "jsonwebtoken": "9.0.0",
    "lodash": "4.17.21",
    "morgan": "1.10.0",
    "passport": "0.6.0",
    "passport-google-oauth20": "2.0.0",
    "rate-limit-redis": "3.0.0",
    "uuid": "8.3.2",
    "validator": "13.9.0",
    "winston": "3.8.0",
    "winston-daily-rotate-file": "4.7.1"
  },
  "devDependencies": {
    "@types/compression": "1.7.2",
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/jest": "29.0.0",
    "@types/jsonwebtoken": "9.0.1",
    "@types/morgan": "1.9.3",
    "@types/node": "18.15.11",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "eslint": "8.37.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-jest": "27.2.1",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "nodemon": "2.0.22",
    "prettier": "2.8.0",
    "prisma": "4.12.0",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}